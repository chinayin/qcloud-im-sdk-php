{
  "name": "chinayin/qcloud-im-sdk-php",
  "description": "Qcloud IM SDK for PHP - 腾讯云IM SDK",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": [
    "sdk",
    "cloud",
    "qcloud",
    "tencent",
    "library",
    "im"
  ],
  "support": {
    "source": "https://github.com/chinayin/qcloud-im-sdk-php",
    "issues": "https://github.com/chinayin/qcloud-im-sdk-php/issues"
  },
  "authors": [
    {
      "name": "chinayin",
      "email": "whereismoney@qq.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-dom": "*",
    "ext-json": "*",
    "doctrine/collections": "^1.5",
    "guzzlehttp/guzzle": "^6.5|^7.0",
    "mockery/mockery": "^1.3",
    "monolog/monolog": "^2.1",
    "psr/simple-cache": "^1.0",
    "symfony/cache": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/http-foundation": "^4.2|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phploc/phploc": "^5.0",
    "sebastian/phpcpd": "^4.0",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "QcloudIM\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "QcloudIM\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-ci": "vendor/bin/phpunit --coverage-text",
    "phpfix": "vendor/bin/php-cs-fixer --exclude=vendor --exclude=tests ./",
    "phploc": "vendor/bin/phploc --exclude=vendor --exclude=tests ./",
    "phpcpd": "vendor/bin/phpcpd --exclude=vendor --exclude=tests ./",
    "phpstan": "vendor/bin/phpstan analyse",
    "travis": [
      "composer phploc",
      "composer phpstan"
    ]
  }
}
